{
   "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "es5",
        "es6"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strict": false,
      "skipLibCheck": true,
   },
   "include": ["./server/models/*.ts"],
   "exclude": [
    "node_modules"
  ]
}